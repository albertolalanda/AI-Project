package eightpuzzle;

/**
 *
 * @author Pedro
 */
class Position {
    private int column;
    private int line;

    public Position(int column, int line) {
        this.column = column;
        this.line = line;
    }

    public int getColumn() {
        return column;
    }

    public void setColumn(int column) {
        this.column = column;
    }

    public int getLine() {
        return line;
    }

    public void setLine(int line) {
        this.line = line;
    }
    
}

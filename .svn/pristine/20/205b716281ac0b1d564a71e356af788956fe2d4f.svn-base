package agent;


public abstract class Action <S extends State>{
    private double cost; // em alguns problemas o custo pode mudar exemplo caminhos que custem mais a percorrer a pe de carro etc neste programa Ã© sempre 1

    public Action(double cost){
        this.cost = cost;
    }

    public abstract void execute(S State);

    public abstract boolean isValid(S State);

    public double getCost(){
        return cost;
    }
}